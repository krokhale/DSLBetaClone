class PagesController < ApplicationController
  def home
    if signed_in?
      @micropost= Micropost.new 
      @feed_items = @micropost.feed.paginate(:page => params[:page])
    end
      @title = "Home"
  end

  def contact
     @title = "Contact"
  end
  
  def about
     @title = "About"
  end
  
  def help
     @title = "Help"
  end
end
